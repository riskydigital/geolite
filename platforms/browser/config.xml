<?xml version='1.0' encoding='utf-8'?>
<widget id="id.riskydigital.Geolite" version="0.2.1" versionCode="3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Geolite</name>
    <description>
    Geolite App untuk mengontrol Pompa
  </description>
    <author email="riskydigital@gmail.com" href="http://goji.web.id">
    Rizki Noor Hidayat Wijaya
  </author>
    <content src="index.html" />
    <icon src="www/img/hdpi.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/img/hdpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/img/mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/img/hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/img/xhdpi.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="www/img/xxhdpi.png" />
    <icon gap:platform="android" gap:qualifier="xxxhdpi" src="www/img/xxxhdpi.png" />
    <access origin="*" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="permissions" value="none" />
</widget>
